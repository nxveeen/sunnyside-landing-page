-content: space-between;\r\n    height: 100px;\r\n    width: 100%;\r\n  }\r\n\r\n  .header__img {\r\n    position: absolute;\r\n    left: 0;\r\n    width: 100%;\r\n    z-index: -1;\r\n    height: 33.5rem;\r\n    object-fit: cover;\r\n  }\r\n\r\n  .ham__icon {\r\n    cursor: pointer;\r\n  }\r\n\r\n  .mobile__links {\r\n    position: absolute;\r\n    top: 140px;\r\n    right: 0;\r\n    left: 0;\r\n    width: 84%;\r\n    // height: 20rem;\r\n    margin: auto;\r\n    display: flex;\r\n    justify-content: center;\r\n    background-color: white;\r\n    z-index: 99;\r\n    border-radius: 4px;\r\n  }\r\n\r\n  .mobile__links ul {\r\n    padding: 20px 0;\r\n    width: 100%;\r\n\r\n    > li {\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      height: 3rem;\r\n      margin-bottom: 20px;\r\n      font-size: 1.5rem;\r\n\r\n      &:last-child {\r\n        background-color: $yellow;\r\n        height: 4rem;\r\n        width: 40%;\r\n        margin: auto;\r\n        margin-top: 30px;\r\n        border-radius: 33px;\r\n\r\n        > a {\r\n          font-size: 1.25rem;\r\n          font-family: \"Fraunces\", serif;\r\n          color: $dark-blue;\r\n          text-transform: uppercase;\r\n        }\r\n      }\r\n\r\n      > a {\r\n        color: $dark-grayish-blue;\r\n        text-align: center;\r\n      }\r\n    }\r\n  }\r\n\r\n  .title {\r\n    margin-top: 3.5rem;\r\n    text-align: center;\r\n    font-family: \"Fraunces\", serif;\r\n    font-weight: 900;\r\n    font-size: 1.5rem;\r\n    color: $white;\r\n    letter-spacing: 3px;\r\n\r\n    @include breakpoint(\"tablet\") {\r\n      font-size: 2rem;\r\n    }\r\n  }\r\n\r\n  .arrow__container {\r\n    margin-top: 4rem;\r\n    width: 100%;\r\n    display: flex;\r\n    justify-content: center;\r\n    animation: upDown ease-in infinite;\r\n\r\n    .arrow__down {\r\n      height: 7rem;\r\n\r\n      &__animate {\r\n        animation: 2s cubic-bezier(0.39, 0.58, 0.57, 1) infinite pulse;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n//----------ANIMATIONS----------\r\n@keyframes pulse {\r\n  0% {\r\n    transform: translateY(-20px);\r\n  }\r\n\r\n  50% {\r\n    transform: translateY(10px);\r\n  }\r\n\r\n  100% {\r\n    transform: translateY(-20px);\r\n  }\r\n}\r\n","src\\sass\\_mixins.scss":"@mixin breakpoint($point) {\r\n  @if $point == \"desktop\" {\r\n    @media (min-width: 70em) {\r\n      @content;\r\n    }\r\n  } @else if $point == \"tablet\" {\r\n    @media (min-width: 50em) {\r\n      @content;\r\n    }\r\n  } @else if $point == \"mobile\" {\r\n    @media (min-width: 37.5em) {\r\n      @content;\r\n    }\r\n  }\r\n}\r\n","src\\sass\\_sections.scss":".section {\r\n  display: flex;\r\n  flex-direction: column;\r\n\r\n  .sect {\r\n    width: 100%;\r\n    overflow: hidden;\r\n\r\n    img {\r\n      width: 100%;\r\n    }\r\n\r\n    .desc {\r\n      padding: 3rem 1.5rem;\r\n      text-align: center;\r\n      height: auto;\r\n\r\n      h1 {\r\n        font-family: \"Fraunces\", serif;\r\n        font-size: 2rem;\r\n        color: $very-dark-desaturated-blue;\r\n        margin-bottom: 3rem;\r\n      }\r\n\r\n      p {\r\n        color: $grayish-blue;\r\n        line-height: 1.6;\r\n        margin-bottom: 2rem;\r\n      }\r\n\r\n      .learn__more {\r\n        position: relative;\r\n        display: block;\r\n        font-family: \"Fraunces\", serif;\r\n        color: $very-dark-desaturated-blue;\r\n        cursor: pointer;\r\n\r\n        &::before {\r\n          content: \"\";\r\n          position: absolute;\r\n          left: -3px;\r\n          right: 0;\r\n          margin: auto;\r\n          top: 14px;\r\n          display: flex;\r\n          height: 6px;\r\n          width: 7rem;\r\n          background-color: rgb(238, 238, 148);\r\n          border-radius: 4px;\r\n          z-index: -1;\r\n        }\r\n\r\n        &__2 {\r\n          &::before {\r\n            background-color: #fdb1a7;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n  .services {\r\n    position: relative;\r\n    margin-top: -5px;\r\n\r\n    .content {\r\n      position: absolute;\r\n      bottom: 4rem;\r\n      display: flex;\r\n      flex-direction: column;\r\n      align-items: center;\r\n      padding: 0 1rem;\r\n\r\n      h1 {\r\n        font-family: \"Fraunces\", serif;\r\n        margin-bottom: 2rem;\r\n        color: $dark-desaturated-cyan;\r\n        font-size: 2rem;\r\n      }\r\n      p {\r\n        text-align: center;\r\n        line-height: 1.4;\r\n        color: $dark-desaturated-cyan;\r\n      }\r\n\r\n      .phtg {\r\n        color: $dark-blue;\r\n      }\r\n    }\r\n  }\r\n}\r\n","src\\sass\\_testimonials.scss":".testm {\r\n  padding: 2rem 1rem;\r\n  width: 100vw;\r\n\r\n  h1 {\r\n    text-align: center;\r\n    font-family: \"Fraunces\", serif;\r\n    text-transform: uppercase;\r\n    font-size: 1rem;\r\n    letter-spacing: 4px;\r\n    color: $grayish-blue;\r\n  }\r\n\r\n  .testimonial {\r\n    display: flex;\r\n    flex-direction: column;\r\n    // justify-content: center;\r\n    align-items: center;\r\n    margin: 4rem 0;\r\n    gap: 1.5rem;\r\n\r\n    img {\r\n      border-radius: 50%;\r\n      width: 6rem;\r\n    }\r\n\r\n    p {\r\n      text-align: center;\r\n      line-height: 1.6;\r\n      color: $dark-grayish-blue;\r\n      padding: 0 1rem;\r\n    }\r\n\r\n    h3 {\r\n      display: flex;\r\n      flex-direction: column;\r\n      align-items: center;\r\n      font-family: \"Fraunces\", serif;\r\n      color: $very-dark-desaturated-blue;\r\n      gap: 0.8rem;\r\n      font-weight: 900;\r\n\r\n      span {\r\n        font-size: 1rem;\r\n        color: $grayish-blue;\r\n        font-family: \"Barlow\", serif;\r\n      }\r\n    }\r\n  }\r\n}\r\n","src\\sass\\_footer.scss":"footer {\r\n  width: 100vw;\r\n  //   padding: 2rem 0;\r\n\r\n  .grid {\r\n    display: grid;\r\n    grid-template-columns: repeat(2, 1fr);\r\n    width: 100%;\r\n\r\n    img {\r\n      width: 100%;\r\n    }\r\n  }\r\n\r\n  .bottom__footer {\r\n   